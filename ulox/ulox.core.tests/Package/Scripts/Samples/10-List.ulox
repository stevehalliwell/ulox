// standard classes library provides a simple list type
build bind "StandardClassesLibrary";

var list = [];
var len = 5;

for (var i = 0; i < len; i+= 1)
{
    list.Add(i);
}

var success = list.Remove(3);

if(!success) print("Found no 3 to remove.");

var length = list.Count();


print("Length is " + length);
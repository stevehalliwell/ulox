// while loops
var i = 0;
while(i < 2)
{
    print ("hip, ");
    i = i + 1;
}

print ("hurray");

// for loops
for(var i = 0; i < 2; i += 1)
{
    print ("hip, ");
}

print ("hurray");

// loop loops
var j = 0;
loop
{
    if(j >= 2)
    {
        break;
    }
    print ("hip, ");
    j = j + 1;
}

print("hurray");
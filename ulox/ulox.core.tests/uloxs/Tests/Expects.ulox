testset ExpectUsageTests
{
    test AreEqual
    {
        expect 1 == 1;
    }
    
    test AreNotEqual
    {
        expect 1 != 2;
    }
    
    test IsTrue
    {
        expect tcname == "IsTrue";
    }
    
    test IsFalse
    {
        expect tcname != "IsTrue";
    }
    
    test IsNull
    {
        var a;
        expect a == null;
    }
    
    test IsNotNull
    {
        var a = 1;
        expect a != null;
    }
}
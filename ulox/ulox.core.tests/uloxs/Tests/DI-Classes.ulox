class ConcreteService 
{
    Meth(){retval =1;}
}

class Dependent
{
    var service = inject Service;
    Get{retval =this.service.Meth();}
}

test DISClassesTests
{
    testcase RegisterAndInject
    {
        register Service ConcreteService();
        var expected = 1;
        var result = 0;

        var dep = Dependent();
        result = dep.Get();

        Assert.AreEqual(expected, result);        
    }
}

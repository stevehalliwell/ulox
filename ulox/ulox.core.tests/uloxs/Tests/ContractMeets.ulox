class A
{
    var a,b,c;
}

class B
{
    var a,b,c,d;
}

class C
{
    AMethod(){}
}

testset ContractMeetsTests
{
    test AMeetsB_False
    {
        var res = null;
        var a = A();
        var b = B();

        res = a meets b;

        Assert.IsFalse(res);
    }

    test BMeetsA_True
    {
        var res = null;
        var a = A();
        var b = B();

        res = b meets a;

        Assert.IsTrue(res);
    }

    test ASignsB_Throws
    {

        fun Act()
        {
            A signs B;
        }

        Assert.Throws(Act);
    }

    test BSignsA_NoThrow
    {

        B signs A;

        Assert.Pass();
    }

    test aMeetsC_False
    {
        var res = null;
        var a = A();

        res = a meets C;

        Assert.IsFalse(res);
    }

    test aMeetsNativeList_False
    {
        var res = null;
        var a = A();
        var list = [];

        res = a meets list;

        Assert.IsFalse(res);
    }

    test DynMeetsDyn_SameFieldButDifferentType_False
    {
        var res = null;
        var a = {:};
        a.a = 3;
        var b = {:};
        b.a = "3";

        res = a meets b;

        Assert.IsFalse(res);
    }
}

fun CreateLevel()
{
    var generator = GridGenerator(Vec2.Create(0,30), Vec2.Create(6,3.5));
    var posList = generator.FromRowCol(2,10);

    loop(posList)
    {
        if(i % 2 == 0)
        {
            item.level = 2;
        }
    }
    
    var levelCreator = LevelCreator();
    LevelCreator.CreateBricksFromPositions(posList);
}